{
  "name": "vue-lazyload-imgs",
  "description": "This Vue component vue lazyload imgs is used to implement lazy loading of images in Vue2 (Vue>=2.6.0) and Vue3 framework environments. The difference from some lazy loading image instructions is that this component can lazily load any number of images at a time, and can also lazily load v-html instructions to render images in HTML.",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "switch": "node ./switch-vue.js"
  },
  "dependencies": {
    "nanoid": "^5.0.9",
    "vue": "^3.5.13",
    "vue-demi": "^0.14.10",
    "vue-lazyload-imgs": "./pkg"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "sass": "^1.83.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.4.0",
    "vue-tsc": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitcode.com/Samdy-Chan/vue-lazyload-imgs.git"
  },
  "keywords": [
    "vue-lazy-load",
    "vue-lazyload-imgs",
    "vue2",
    "vue3",
    "lazy",
    "load",
    "lazyLoadImgs",
    "lazy-load-imgs",
    "lazyLoadImgages",
    "lazy-load-imgages",
    "懒加载",
    "懒加载图片",
    "延迟加载",
    "延迟加载图片"
  ],
  "author": "Samdy_Chan",
  "license": "ISC"
}
